// Consider adding the 1918 zones here, if they are not used in your
// organization
//include "/etc/bind/zones.rfc1918";

zone "lab.mmkbmss.com" IN {
        type master;
        file "/etc/bind/forward.lab.mmkbmss.com";
        allow-update { none; };
};
zone "101.168.192.in-addr.arpa" IN {
        type master;
        file "/etc/bind/reverse.lab.mmkbmss.com";
        allow-update { none; };
};